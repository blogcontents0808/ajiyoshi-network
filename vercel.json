{
  "routes": [
    {
      "src": "/(blog|activities|members|history|contact|privacy|terms|thanks|data-deletion)\\.html$",
      "dest": "/$1.html",
      "headers": {
        "cache-control": "s-maxage=86400"
      }
    },
    {
      "src": "/index\\.html$",
      "dest": "/index.html",
      "headers": {
        "cache-control": "s-maxage=86400"
      }
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|ico|svg|json))$", 
      "dest": "/$1",
      "headers": {
        "cache-control": "s-maxage=31536000"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/",
      "dest": "/"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options", 
          "value": "DENY"
        }
      ]
    }
  ]
}