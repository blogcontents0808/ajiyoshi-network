{
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/index.html", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}